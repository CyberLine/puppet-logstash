{
  "name": "elastic-logstash",
  "version": "7.0.0",
  "author": "elastic",
  "summary": "Module for managing and configuring Logstash",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-logstash",
  "project_page": "https://github.com/elastic/puppet-logstash",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0 <9.0.0"
